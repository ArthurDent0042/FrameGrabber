{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],

  "UseConfigInsteadOfCommandLine": false,    // Use the values here instead of reading from the commandline
  "autoGenerate": "2",                      // How many screenshots should be generated
  "CaptureSpecificFrames": "",              // Use if you want to capture specific frames. Comma delimited list "900, 1500, 8993"
  "DisplayOverlay": true,                   // Display an overlay of text onto the image
  "DisplayFrameNumber": true,               // Overlay the frame number onto the screenshot? true/false
  "labels": "Label1 1, Label1 2",           // Text to be displayed underneath the frame number text. If multiple, must be comma separated. ex: "Label1, Label2"
                                            // The number of elements listed here must be the same as the number of files being processed
  "labelsRow2": "Label2 1, Label2 2",       // Second line of labels
  "DisplayFrameType":  true,                // Display the picture frame type on the overlay
  "DisplayTimeStamp": true,                 // Display the timestamp on the overlay
  "fontColor": "White",                     // Font color of the overlaid text. Hex values may also be used
  "fontSize": 15,                           // Font size of the overlaid text
  "boxColor": "Black",                      // Color of the box drawn around the overlaid text. Hex values may also be used
  "boxOpacity": 1.0,                        // Opacity of the box drawn around the overlaid text. Value is between 0.0 and 1.0
  "x-Offset": 0,                            // Number of pixels to offset the overlay on the x-axis
  "y-Offset": 0                             // Number of pixels to offset the overlay on the y-axis
}

